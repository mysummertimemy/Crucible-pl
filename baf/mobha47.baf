  RESPONSE #100
    SetGlobalTimer("MO_SpellCastTimer","LOCALS",6)
    SetGlobalTimer("MO_EvilEye","LOCALS",12)
    SetInterrupt(FALSE)
    ForceSpellRES("mobha47a",LastSeenBy(Myself))
    SetInterrupt(TRUE)
END

